L1_SCALEIN_ENABLE=true
L1_SCALEIN_MIN_QUOTE_USDT=5.0
L1_SCALEIN_MAX_STEPS_PER_DAY=3
L1_SCALEIN_FR_BUFFER=0.0

# === Debug/Logging ===
TRACE_API=false
EXTRA_LOGS=true
